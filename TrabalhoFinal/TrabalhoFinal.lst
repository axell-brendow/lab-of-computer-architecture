                      00071 ;       BSF TRISA, 2
                      00072 ;       CLRF INTCON ; TODAS AS INTERRUPÇÕES DESLIGADAS
                      00073 ;
                      00074 ;       ; BCF LIMPA O BIT RP0(5) DO REGISTRADOR STATUS,
                      00075 ;       ; OU SEJA, VOLTAMOS AO BANCO DE MEMÓRIA 00.
                      00076 ;       BCF STATUS, RP0
                      00077 ;       MOVLW B'00000111'
