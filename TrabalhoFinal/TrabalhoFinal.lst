                      00054 ;       ; STATUS = IRP  RP1     RP0     TO      PD      Z       DC      C
                      00055 ;       ; B6 E B5 SERVEM PARA SELECIONAR UM DOS 4
                      00056 ;       ; BANCOS DE MEMÓRIA.
                      00057 ;       ; Z = ZERO FLAG
                      00058 ;       ; DC = DIGIT CARRY
                      00059 ;       ; C = CARRY
                      00060 ;       BSF STATUS, RP0
                      00061 ;       ; AGORA, OS BITS DE SELEÇÃO DO BANCO DE
                      00062 ;       ; MEMÓRIA FORMAM O NUMERO 01, POIS
                      00063 ;       ; RP1 = 0 (ESSE É O BIT 6 DO STATUS)
                      00064 ;       ; RP0 = 1 (BSF LIGOU O BIT RP0 (BIT 5))
                      00065 ;       ; QUE EQUIVALE A ACESSAR O BANCO DE MEMÓRIA 01
                      00066 ;
                      00067 ;       ; CLRF LIMPA O REGISTRADOR TRISB.
                      00068 ;       ; O REGISTRADOR TRISB ESTÁ NO BANCO 1
                      00069 ;       CLRF TRISB
