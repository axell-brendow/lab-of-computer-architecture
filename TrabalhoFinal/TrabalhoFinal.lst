                      00001 ;#INCLUDE <P16F628A.INC>                ;ARQUIVO PADRÃO MICROCHIP PARA 16F628A
                      00002 ;;RADIX DEC
                      00003 ;__CONFIG _CP_OFF & _LVP_OFF & _BOREN_OFF & _MCLRE_ON & _WDT_OFF & _PWRTE_ON & _INTOSC_OSC_NOCLKOUT
                      00004 ;;__CONFIG H'3F10'
                      00005 ;
                      00006 ;; O BOTAO SERÁ CONECTADO NO TERCEIRO BIT DE PORTA
                      00007 ;#DEFINE BTN PORTA, 2
                      00008 ;#DEFINE LED_C PORTB, 5 ; LED CONTROLE
                      00009 ;#DEFINE LED_A4 PORTB, 4 ; LED ANDAR 4
                      00010 ;#DEFINE LED_A3 PORTB, 3 ; LED ANDAR 3
                      00011 ;#DEFINE LED_A2 PORTB, 2 ; LED ANDAR 2
                      00012 ;#DEFINE LED_A1 PORTB, 1 ; LED ANDAR 1
                      00013 ;#DEFINE LED_T PORTB, 0 ; LED TÉRREO
                      00014 ;
                      00015 ;       ; AS VARIÁVEIS ABAIXO SERÃO RESERVADAS A
                      00016 ;       ; PARTIR DO ENDEREÇO 0X20, QUE É UM ENDEREÇO
                      00017 ;       ; DO BANCO ZERO DO PIC
                      00018 ;       CBLOCK  0x20
                      00019 ;       CONTADOR1
                      00020 ;       CONTADOR2
                      00021 ;       CONTADOR3
                      00022 ;       CONTADOR_DE_DELAYS
                      00023 ;       ENDC    ;FIM DO BLOCO DE MEMÓRIA
                      00024 ;       
                      00025 ;       ; AS INSTRUÇÕES ABAIXO SERÃO COLOCADAS A
                      00026 ;       ; PARTIR DO ENDEREÇO DE MEMÓRIA 0X00. É
                      00027 ;       ; IMPORTANTE NOTAR QUE A ARQUITETURA DO PIC
                      00028 ;       ; É DE HARVARD, OU SEJA, O ENDEREÇO 0X20 DA
                      00029 ;       ; DIRETIVA CBLOCK SE REFERE A UMA MEMÓRIA
                      00030 ;       ; DIFERENTE DO ENDEREÇO 0X00 DA DIRETIVA ORG.
                      00031 ;       ORG     0x00
                      00032 ;       ; ESTA INSTRUÇÃO FICARÁ NO PRIMEIRO ENDEREÇO
                      00033 ;       ; DA MEMÓRIA PARA PROGRAMA. CASO QUEIRA
                      00034 ;       ; VISUALIZAR A MEMÓRIA PARA PROGRAMA DO PIC:
                      00035 ;       ; (View -> Program Memory -> Symbolic)
                      00036 ;       GOTO    INICIO
                      00037 ;
                      00038 ;       ; LINKS ÚTEIS:
                      00039 ;       ; https://en.wikipedia.org/wiki/PIC_instruction_listings
                      00040 ;       ; https://pictutorials.com/The_Status_Register.htm
                      00041 ;       ; DATASHEET:
                      00042 ;       ; http://web.mit.edu/6.115/www/document/16f628.pdf
                      00043 ;
                      00044 ;INICIO
                      00045 ;       CLRF    PORTA           ; LIMPA O PORTA
                      00046 ;       CLRF    PORTB           ; LIMPA O PORTB
                      00047 ;       ; RP0 = REGISTER PAGE Bit 0
                      00048 ;       ; RP0 É UM BIT DE SELEÇÃO DE BANCO DE MEMÓRIA
                      00049 ;       ; RP0 É O SEXTO BIT DO REGISTRADOR STATUS
                      00050 ;       ; "BSF STATUS, RP0" É IGUAL A "BSF, STATUS, 5"
                      00051 ;       ; QUE EQUIVALE A COLOCAR O SEXTO BIT DE STATUS
                      00052 ;       ; COMO 1 (BSF = BIT SET FILE)
                      00053 ;       ;          B7   B6      B5      B4      B3      B2      B1      B0
